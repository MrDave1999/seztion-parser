{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "src/SeztionParser/**.csproj"
          ]
        }
      ],
      "properties": 
       { 
           "TargetFramework": "netstandard2.0"
       },
      "dest": "api",
      "filter": "filterConfig.yml",  
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "diagrams/**.md",
          "diagrams/**/toc.yml",
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "seztion-parser",
      "_enableNewTab": true,
      "_appFaviconPath": "images/seztionparser-icon.ico",
      "_appFooter": "Copyright 2021 <b>seztion-parser</b>",
      "_enableSearch": true,
      "_gitContribute": {
        "repo": "https://github.com/MrDave1999/seztion-parser",
        "branch": "main"
      }
    },
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "plugins/memberpage/content"
    ],
    "postProcessors": ["ExtractSearchIndex"],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ]
  }
}